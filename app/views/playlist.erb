<ul>
  <% Playlist.sort.each do |song| %>
  <li> <%= song.upvote_count %> - <a href="/songs/<%= song.slug %>/upvote"> Upvote </a>   <%= song.spotify_id %>  </li>
  <% end %>
</ul>
<br>

<form action="/spotify" method="get">
  <input type="text" name="search" placeholder="enter something">
  <input type="submit" value="search">
</form>


